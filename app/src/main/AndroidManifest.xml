<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kakao.uidemo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PopActivity"
            android:label="@string/title_activity_pop" />
        <activity
            android:name=".ToastActivity"
            android:label="@string/title_activity_toast" />
        <activity
            android:name=".WheelActivity"
            android:label="@string/title_activity_wheel" />
        <activity
            android:name=".DialogActivity"
            android:label="@string/title_activity_dialog" />
        <activity
            android:name=".AlignTextActivity"
            android:label="@string/title_activity_align_text" />
        <activity
            android:name=".TitleBarActivity"
            android:label="@string/title_activity_title_bar"></activity>
    </application>

</manifest>